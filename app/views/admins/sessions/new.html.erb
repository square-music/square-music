<section class="admin_login_container ,clearfix">
  <div class="headline_box">
    <h2 class="login_headline">管理者ログイン</h2>
  </div>
  <table border="1" class="login_table">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <tr>
      <div class="field">
        <td class="td_color"><%= f.label:メールアドレス  %><br/></td>
        <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
      </div>
    </tr>
    <tr>
      <div class="field">
        <td class="td_color"><%= f.label:パスワード %><br /></td>
        <td><%= f.password_field :password, autocomplete: "off" %></td>
      </div>
    </tr>
  </table>
  <div class="actions">
    <%= f.submit "ログイン",class: "login_btn" %>
  </div>
  <% end %>
  <%= render "users/shared/links" %>
</section>
